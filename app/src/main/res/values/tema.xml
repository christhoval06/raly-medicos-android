<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="Theme.Raly_medicos" parent="@style/Theme.Sherlock.Light">
        <item name="actionBarItemBackground">@drawable/selectable_background_raly_medicos</item>
        <item name="popupMenuStyle">@style/PopupMenu.Raly_medicos</item>
        <item name="dropDownListViewStyle">@style/DropDownListView.Raly_medicos</item>
        <item name="actionBarTabStyle">@style/ActionBarTabStyle.Raly_medicos</item>
        <item name="actionDropDownStyle">@style/DropDownNav.Raly_medicos</item>
        <item name="actionBarStyle">@style/ActionBar.Solid.Raly_medicos</item>
        <item name="actionModeBackground">@drawable/cab_background_top_raly_medicos</item>
        <item name="actionModeSplitBackground">@drawable/cab_background_bottom_raly_medicos</item>
        <item name="actionModeCloseButtonStyle">@style/ActionButton.CloseMode.Raly_medicos</item>
    </style>

    <style name="ActionBar.Solid.Raly_medicos" parent="@style/Widget.Sherlock.Light.ActionBar.Solid">
        <item name="background">@drawable/ab_solid_raly_medicos</item>
        <item name="backgroundStacked">@drawable/ab_stacked_solid_raly_medicos</item>
        <item name="backgroundSplit">@drawable/ab_bottom_solid_raly_medicos</item>
        <item name="progressBarStyle">@style/ProgressBar.Raly_medicos</item>
    </style>

    <style name="ActionBar.Transparent.Raly_medicos" parent="@style/Widget.Sherlock.Light.ActionBar">
        <item name="background">@drawable/ab_transparent_raly_medicos</item>
        <item name="progressBarStyle">@style/ProgressBar.Raly_medicos</item>
    </style>

    <style name="PopupMenu.Raly_medicos" parent="@style/Widget.Sherlock.Light.ListPopupWindow">
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_raly_medicos</item>
    </style>

    <style name="DropDownListView.Raly_medicos" parent="@style/Widget.Sherlock.Light.ListView.DropDown">
        <item name="android:listSelector">@drawable/selectable_background_raly_medicos</item>
    </style>

    <style name="ActionBarTabStyle.Raly_medicos" parent="@style/Widget.Sherlock.Light.ActionBar.TabView">
        <item name="android:background">@drawable/tab_indicator_ab_raly_medicos</item>
    </style>

    <style name="DropDownNav.Raly_medicos" parent="@style/Widget.Sherlock.Light.Spinner.DropDown.ActionBar">
        <item name="android:background">@drawable/spinner_background_ab_raly_medicos</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_raly_medicos</item>
        <item name="android:dropDownSelector">@drawable/selectable_background_raly_medicos</item>
    </style>

    <style name="ProgressBar.Raly_medicos" parent="@style/Widget.Sherlock.Light.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_raly_medicos</item>
    </style>

    <style name="ActionButton.CloseMode.Raly_medicos" parent="@style/Widget.Sherlock.Light.ActionButton.CloseMode">
        <item name="android:background">@drawable/btn_cab_done_raly_medicos</item>
    </style>

    <!-- this style is only referenced in a Light.DarkActionBar based theme -->
    <style name="Theme.Raly_medicos.Widget" parent="@style/Theme.Sherlock">
        <item name="popupMenuStyle">@style/PopupMenu.Raly_medicos</item>
        <item name="dropDownListViewStyle">@style/DropDownListView.Raly_medicos</item>
    </style>

</resources>